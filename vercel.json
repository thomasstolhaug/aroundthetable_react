{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
        }
    ],
    "rewrites": [
        {
        "source": "/api/(.*)",
        "destination": "https://aroundthewebapp-hydncfeeemfvcqga.norwayeast-01.azurewebsites.net/api/:path*"
        },
        {
        "source": "/(.*)",
        "destination": "/index.html"
        }
    ]
}
